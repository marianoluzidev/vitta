    "material-icons": "^1.13.14",
    "skeleton-elements": "^4.0.1",
    "swiper": "^12.0.3",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/compiler-sfc": "^3.5.26",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.1.0",
    "firebase-admin": "^13.6.0",
    "minimist": "^1.2.8",
    "postcss-preset-env": "^10.6.0",
